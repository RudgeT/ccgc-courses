@inherits LayoutComponentBase

@inject IStringLocalizer<Wet> Localizer

<WetHeader />

<main role="main" property="mainContentOfPage" class="container">
    <h1 property="name" id="wb-cont">@Localizer["AppTitle"]</h1>
    <p class="gc-byline"><strong>@((MarkupString)Localizer["gc-byline"].Value)</strong></p>
    <section>

        @Body

    </section>

    <div class="pagedetails">
        <dl id="wb-dtmd">
            <dt>@Localizer["DateModified"]:&#32;</dt>
            <dd><time property="dateModified">2020-09-25</time></dd>
        </dl>

    </div>

</main>

<footer role="contentinfo" id="wb-info">
    <WetFooter />
</footer>


